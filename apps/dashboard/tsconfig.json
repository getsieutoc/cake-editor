{
  "extends": "@sieutoc/tsconfig/nextjs.json",
  "compilerOptions": {
    "outDir": "dist",
    "incremental": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules", "build"
  ]
}
